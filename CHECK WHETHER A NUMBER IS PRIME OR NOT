//PRIME OR NOT
#include<stdio.h>
int main()
{
    int num,i,flag=0;
    printf("\n ENTER THE NUMBER:");
    scanf("%d",&num);
    for(i=2;i<=num/2;++i)
    {
        if(num%i==0)
        {
            flag=1;
            break;
        }    
    }
    if(flag==1)
        printf("neither prime nor composite");
    else if(flag==0) 
        printf(" prime");
    else
        printf("\n not prime");
    return 0;
}    
