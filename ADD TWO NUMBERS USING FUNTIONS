#include<stdio.h>
int accept()
{
    int num1;
    printf("\n ENTER THE VALUES OF num:");
    scanf("%d",&num1);
    return num1;
}
int sum(int a,int b)
{
    int sum;
    sum=a+b;
    
}
int main()
{
    int a,b,c;
    a=accept();
    b=accept();
    c=sum(a,b);
    printf("\n THE ADDITION OF TWO NUMBERS A AND B IS:%d",c);
}
