#include <stdio.h>
#include <string.h>
#include <stdlib.h>
int main()
{
    char *s;int i;
    s=(char *)malloc(sizeof(char));
    printf("\n enter the name: ");
    gets(s);
    for(i=0;s[i]!='\0';i++)
    {
        if(s[i]!=' ')
            printf("%c",s[i]);
        else 
            printf("\n");
    }
    return 0;
}
